{
	"permissions": "*",
	"routing": "routing.json",
	"frontend":"frontend",
	"autoload": {
		"directories": ["controllers", "libraries", "listeners", "processes", "views"]
	},
	"languages":{
		"fa_IR" : "langs/fa_IR.json",
		"en_US" : "langs/en_US.json"
	},
	"events": [
		{
			"name":"packages/userpanel/Events/UserTypePermissionsList",
			"listener": "packages/request/Listeners/Settings/UserType@permissions_list"
		},
		{
			"name":"packages/userpanel/Events/Search",
			"listener": "packages/request/Listeners/Search@find"
		},
		{
			"name":"packages/userpanel/Events/Users/BeforeDelete",
			"listener": "packages/request/Listeners/UserPanel/Users/BeforeDelete@check"
		},
		{
			"name":"packages/notifications/Events",
			"listener": "packages/request/Listeners/Notifications@events"
		}
	]
}
